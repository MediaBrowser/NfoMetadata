  </data>
  <data name="LabelKodiMetadataEnableExtraThumbs" xml:space="preserve">
    <value>Kopiera extrafanart till extrathumbs</value>
    
  </data>
  <data name="LabelKodiMetadataEnableExtraThumbsHelp" xml:space="preserve">
